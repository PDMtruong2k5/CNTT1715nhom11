{var width=$this.parent().width();var className=null;var classPrefix='cbs-width-';if(width>=1170)className='1170';else if(width>=960)className='960';else if(width>=768)className='768';else if(width>=480)className='480';else if(width>=300)className='300';else if(width>=0)className='0';var oldClassName=$self.getValueFromClass($this,classPrefix);if(oldClassName!==false)$this.removeClass(classPrefix+oldClassName);$this.addClass(classPrefix+className);if($self.prevWidth!==width)
{$self.prevWidth=width;$self.calculateCalendarColumnWidth();}
