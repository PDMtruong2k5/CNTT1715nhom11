{};this.moveCalendar=function(event,step)
{event.preventDefault();var calendarBox=$this.find('.cbs-calendar-table-wrapper');var calendar=calendarBox.find('.cbs-calendar');var columnActive=Math.abs($('th.cbs-active').index());var visibleColumns=parseInt($this.find('input[name="cbs-calendar-column-count"]').val());var hiddenColumns;var calendarStep;var activeColumnStep;if(Math.abs(step)<=visibleColumns)
